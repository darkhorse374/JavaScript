{
	"name": "ts-blocks",
	"description": "A CLI to blocks to your project.",
	"version": "1.0.0-next.10",
	"packageManager": "pnpm@9.12.3",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ieedan/ts-blocks"
	},
	"keywords": ["blocks", "typescript", "tested", "documented"],
	"author": "Aidan Bleser",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/ieedan/ts-blocks/issues"
	},
	"homepage": "https://ieedan.github.io/ts-blocks/",
	"main": "bin.mjs",
	"bin": "./bin.mjs",
	"type": "module",
	"files": [
		"bin.mjs",
		"package.json",
		"pnpm-lock.yaml",
		"README.md",
		"schema.json",
		"dist/**/*",
		"src/**/*",
		"blocks/**/*"
	],
	"scripts": {
		"start": "tsup --silent && node bin.mjs",
		"build": "tsup",
		"format": "biome format --write",
		"lint": "biome lint --write",
		"check": "biome check",
		"ci:release": "tsup && changeset publish",
		"changeset": "changeset",
		"test": "vitest"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/node": "^22.9.0",
		"tsup": "^8.3.5",
		"typescript": "^5.6.3",
		"vitest": "^2.1.5"
	},
	"dependencies": {
		"@changesets/cli": "^2.27.9",
		"@clack/prompts": "^0.7.0",
		"chalk": "^5.3.0",
		"commander": "^12.1.0",
		"execa": "^9.5.1",
		"octokit": "^4.0.2",
		"package-manager-detector": "^0.2.2",
		"ts-morph": "^24.0.0",
		"valibot": "^0.42.1"
	}
}
