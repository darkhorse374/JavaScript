{
	"name": "ts-blocks",
	"description": "A CLI to blocks to your project.",
	"version": "0.2.1",
	"packageManager": "pnpm@9.8.0",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ieedan/ts-blocks"
	},
	"keywords": [
		"changelog",
		"date"
	],
	"author": "Aidan Bleser",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/ieedan/ts-blocks/issues"
	},
	"homepage": "https://github.com/ieedan/ts-blocks",
	"main": "bin.mjs",
	"bin": "./bin.mjs",
	"type": "module",
	"files": [
		"bin.mjs",
		"package.json",
		"pnpm-lock.yaml",
		"README.md",
		"schema.json",
		"dist/**/*",
		"src/**/*",
		"blocks/**/*"
	],
	"scripts": {
		"start": "unbuild && node bin.mjs",
		"build": "unbuild",
		"format": "biome format --write",
		"lint": "biome lint --write",
		"check": "pnpm biome check",
		"ci:release": "unbuild && changeset publish",
		"changeset": "changeset",
		"test": "vitest"
	},
	"devDependencies": {
		"@biomejs/biome": "1.8.3",
		"@types/node": "^22.5.4",
		"typescript": "^5.5.3",
		"unbuild": "^2.0.0",
		"vitest": "^2.0.5"
	},
	"dependencies": {
		"@changesets/cli": "^2.27.8",
		"@clack/prompts": "^0.7.0",
		"chalk": "^5.3.0",
		"commander": "^12.1.0",
		"execa": "^9.3.1",
		"package-manager-detector": "^0.2.0",
		"ts-morph": "^23.0.0",
		"valibot": "^0.41.0"
	}
}
